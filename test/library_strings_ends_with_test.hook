
use { ends_with } in strings;

assert(ends_with("", "") == false, "ends_with('', '') == false");
assert(ends_with("foo", "") == false, "ends_with('foo', '') == false");
assert(ends_with("", "foo") == false, "ends_with('', 'foo') == false");
assert(ends_with("foo", "foobar") == false, "ends_with('foo', 'foobar') == false");
assert(ends_with("foobar", "foo") == false, "ends_with('foobar', 'foo') == false");
assert(ends_with("foobar", "bar") == true, "ends_with('foobar', 'bar') == true");
